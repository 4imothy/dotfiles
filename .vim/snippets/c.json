{
    "for": {
        "prefix": "for",
        "body": [
            "for (${1:init}; ${2:condition}; ${3:increment}) {",
                "\t$0",
                "}"
        ]
    },
    "ifnd": {
        "prefix": "ifnd",
        "body": ["#ifndef $1",
            "$0",
            "#endif // $1"
        ]
    },
    "if": {
        "prefix": "if",
        "body": ["if ($1) {",
            "\t$0",
            "}"
        ]
    },
    "elif": {
        "prefix": "elif",
        "body": ["else if ($1) {",
            "\t$0",
            "}"
        ]
    },
    "#include <...>": {
        "prefix": "inc",
        "body": ["#include <$0>"]
    },
    "#include \"...\"": {
        "prefix": "incl",
        "body": ["#include \"$0\""]
    }
}
