{
  "task": {
    "prefix": "task",
    "body": [
      "* ( ) ${1:item} \\`${2:label}\\`"
    ]
  },
  "date task": {
    "prefix": "dask",
    "body": [
      "* ( ) ,${1:date}, ${2:item} \\`${3:label}\\`"
    ]
  },
  "time task": {
    "prefix": "ttask",
    "body": [
      "* ( ) ,${1:date}, \\$${2:time}\\$ ${3:item} \\`${4:label}\\`"
    ]
  }
}
