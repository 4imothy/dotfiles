{
    "task": {
        "prefix": "task",
        "body": [
            "* ( ) ${1:item} `${2:label}`"
        ]
    },
    "dask": {
        "prefix": "dask",
        "body": [
            "* ( ) ,${1:date}, ${2:item} `${3:label}`"
        ]
    },
    "ttask": {
        "prefix": "ttask",
        "body": [
            "* ( ) ,${1:date}, \\$${2:time}\\$ ${3:item} `${4:label}`"
        ]
    }
}
