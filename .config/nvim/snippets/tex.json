{

    "bf": {
        "prefix": "bf",
        "body": [
            "\\textbf{$1}"
        ]
    },

    "em": {
        "prefix": "em",
        "body": [
            "\\emph{$1}"
        ]
    },

    "it": {
        "prefix": "it",
        "body": [
            "\\item $1"
        ]
    },

    "begin": {
        "prefix": "begin",
        "body": [
            "\\begin{${1:environment}}",
            "\\item $0",
            "\\end{$1}"
        ]
    },

    "suml": {
        "prefix": "suml",
        "body": [
            "\\sum\\limits_{$1}^{$2}$3"
        ]
    },

    "sec": {
        "prefix": "sec",
        "body": [
            "\\section*{$1}",
            "",
            "$0"
        ]
    },

    "ssec": {
        "prefix": "ssec",
        "body": [
            "\\subsection*{$1}",
            "",
            "$0"
        ]
    },

    "spread": {
        "prefix": "spread",
        "body": [
            "($1_{$2}, \\ldots, $1_{$4})"
        ]
    },

    "bracks": {
        "prefix": "bracks",
        "body": [
            "\\lbrack{$1}\\rbrack"
        ]
    },


    "angles": {
        "prefix": "angles",
        "body": [
            "\\langle{$1}\\rangle"
        ]
    },


    "bmat": {
        "prefix": "bmat",
        "body": [
            "\\begin{bmatrix}"
        ]
    },


    "emat": {
        "prefix": "emat",
        "body": [
            "\\end{bmatrix}"
        ]
    },

    "assignment": {
        "prefix": "assignment",
        "body": [
            "\\documentclass[article,12pt]{article}",
            "\\usepackage{tims_shorts}",
            "",
            "\\usepackage[left=0.4in, right=0.4in, top=0.4in, bottom=0.4in]{geometry}",
            "",
            "\\title{${1:Assignment Title}}",
            "\\author{Timothy Cronin (tlc107)}",
            "\\date{\\today}",
            "",
            "\\pagenumbering{gobble}",
            "",
            "\\begin{document}",
            "",
            "\\maketitle",
            "",
            "$0",
            "",
            "\\end{document}"
        ],
        "description": "Assignment Template"
    },

    "mR": {
        "prefix": "mR",
        "body": [
            "\\mathbb{R}"
        ]
    },

    "mC": {
        "prefix": "mC",
        "body": [
            "\\mathbb{C}"
        ]
    },

    "mQ": {
        "prefix": "mQ",
        "body": [
            "\\mathbb{Q}"
        ]
    },


    "mN": {
        "prefix": "mN",
        "body": [
            "\\mathbb{N}"
        ]
    },

    "mZ": {
        "prefix": "mZ",
        "body": [
            "\\mathbb{Z}"
        ]
    },


    "mF": {
        "prefix": "mF",
        "body": [
            "\\mathbb{F}"
        ]
    }
}
