{
  "bold text": {
    "prefix": "bf",
    "body": "\\textbf{${1}}"
  },
  "emphasized text": {
    "prefix": "em",
    "body": "\\emph{${1}}"
  },
  "item": {
    "prefix": "it",
    "body": "\\item ${1}"
  },
  "begin environment": {
    "prefix": "begin",
    "body": [
      "\\begin{${1:environment}}",
      "\\item ${0}",
      "\\end{${1}}"
    ]
  },
  "summation": {
    "prefix": "suml",
    "body": "\\sum\\limits_{${1}}^{${2}}${3}"
  },
  "section": {
    "prefix": "sec",
    "body": [
      "\\section*{${1}}",
      "",
      "${0}"
    ]
  },
  "subsection": {
    "prefix": "ssec",
    "body": [
      "\\subsection*{${1}}",
      "",
      "${0}"
    ]
  },
  "subsubsection": {
    "prefix": "sssec",
    "body": [
      "\\subsubsection*{${1}}",
      "",
      "${0}"
    ]
  },
  "spread": {
    "prefix": "spread",
    "body": "(${1}_${2}, \\ldots, ${1}_${4})"
  },
  "square brackets": {
    "prefix": "bracks",
    "body": "\\lbrack{${1}}\\rbrack"
  },
  "angle brackets": {
    "prefix": "angles",
    "body": "\\langle{${1}}\\rangle"
  },
  "begin bmatrix": {
    "prefix": "bmat",
    "body": "\\begin{bmatrix}"
  },
  "end bmatrix": {
    "prefix": "emat",
    "body": "\\end{bmatrix}"
  },
  "assignment template": {
    "prefix": "assignment",
    "body": [
      "\\documentclass[article,12pt]{article}",
      "\\usepackage{tims_shorts}",
      "",
      "\\usepackage[left=0.4in, right=0.4in, top=0.4in, bottom=0.4in]{geometry}",
      "",
      "\\title{${1:Assignment Title}}",
      "\\author{Timothy Cronin (tlc107)}",
      "\\date{\\today}",
      "",
      "\\pagenumbering{gobble}",
      "",
      "\\begin{document}",
      "",
      "\\maketitle",
      "",
      "${0}",
      "",
      "\\end{document}"
    ]
  },
  "mathbb r": {
    "prefix": "mR",
    "body": "\\mathbb{R}"
  },
  "mathbb c": {
    "prefix": "mC",
    "body": "\\mathbb{C}"
  },
  "mathbb q": {
    "prefix": "mQ",
    "body": "\\mathbb{Q}"
  },
  "mathbb n": {
    "prefix": "mN",
    "body": "\\mathbb{N}"
  },
  "mathbb z": {
    "prefix": "mZ",
    "body": "\\mathbb{Z}"
  },
  "mathbb f": {
    "prefix": "mF",
    "body": "\\mathbb{F}"
  }
}
